    {
      "type": "ExpansionPanel",
      "caption": "Functions",
      "items": [
        {
          "type": "Label",
          "caption": "(2) Monitored variables"
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Determine and assign variables automatically",
              "onClick": "HMCPM_DetermineVariables($id);"
            },
            {
              "type": "Button",
              "caption": "Display registered variables",
              "onClick": "$variables = HMCPM_DisplayRegisteredVariables($id); print_r($variables);"
            }
          ]
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Assign missing variable profiles only",
              "onClick": "HMCPM_AssignVariableProfile($id, false);"
            },
            {
              "type": "Button",
              "caption": "Overwrite variable profiles",
              "onClick": "HMCPM_AssignVariableProfile($id, true);"
            }
          ]
        },
        {
          "type": "Label",
          "caption": "(3) Notifications"
        },
        {
          "type": "Label",
          "caption": "Value is below threshold:"
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Display variables",
              "onClick": "$variables = HMCPM_DisplayVariablesBelowThreshold($id); print_r($variables);"
            },
            {
              "type": "Button",
              "caption": "Display blocked variables for today",
              "onClick": "$variables = HMCPM_DisplayBlockedVariablesForTodayBelowThreshold($id); print_r($variables);"
            }
          ]
        },
        {
          "type": "Label",
          "caption": "Threshold reached:"
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "Button",
              "caption": "Display variables",
              "onClick": "$variables = HMCPM_DisplayVariablesThresholdReached($id); print_r($variables);"
            },
            {
              "type": "Button",
              "caption": "Display blocked variables for today",
              "onClick": "$variables = HMCPM_DisplayBlockedVariablesForTodayThresholdReached($id); print_r($variables);"
            }
          ]
        },
